    "build-raw": "vue-cli-service library-build --vusion-mode raw",
    "doc-build:all": "vue-cli-service doc-build  --theme dark,seagreen",
    "build:all": "vue-cli-service library-build  --theme dark,seagreen",
    "lint": "vue-cli-service lint"
