    "shelljs": "^0.7.8",
    "stylelint": "^9.3.0",
    "stylelint-config-vusion": "^1.1.2",
    "vue": "^2.6.10",
    "vue-cli-plugin-vusion": "^0.5.13",
    "vue-template-compiler": "^2.6.10",
    "vusion-hooks": "^0.3.0"
